@model IEnumerable<ExternalUserView>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout_Admin.cshtml";
}

<div class="row">
    <div class="col-md-12 ">
        <div class="element-box element-wrapper">
            <h5 class="form-header">
                Customers List
            </h5>
            <div class="form-desc">
                Manage customers on the portal
                <!--   <a class="btn btn-sm btn-primary float-right" href="#"> <i class="os-icon os-icon-plus-circle"></i> Add Customer</a> -->
            </div>
            <div class="element-box-tp">
                <div class="table-responsive">
                    <table class="table table-striped b-b">
                        <thead>
                            <tr>
                                <th class="text-center">
                                    Membership Number
                                </th>
                                <th>
                                    Name
                                </th>
                                <th>
                                    Email
                                </th>
                                <th>
                                    KYC Status
                                </th>
                                <th class="text-center">
                                    Actions
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {

                                <tr>
                                    <td class="text-center">
                                        <span>@item.MembershipNumber</span>
                                    </td>
                                    <td>
                                        <div class="user-with-avatar">
                                            <img alt="" src="~/custom/img/default-profile.svg"><span>@item.Name</span>
                                        </div>
                                    </td>

                                    <td>
                                        @item.Email
                                    </td>
                                    <td class="nowrap">
                                        <span class="status-pill smaller green"></span><span>Approved</span>
                                    </td>
                                    <td class="row-actions">
                                        <a href="#" title="View"><i class="os-icon os-icon-eye"></i></a>
                                        <a href="#" title="Edit"><i class="os-icon os-icon-edit"></i></a>
                                        <a href="#" title="Suspend"><i class="os-icon os-icon-x-square"></i></a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                    <div class="controls-below-table">
                        <div class="table-records-info">
                            Showing records 1 - 5
                        </div>
                        <div class="table-records-pages">
                            <ul>
                                <li>
                                    <a href="#">Previous</a>
                                </li>
                                <li>
                                    <a class="current" href="#">1</a>
                                </li>
                                <li>
                                    <a href="#">2</a>
                                </li>
                                <li>
                                    <a href="#">3</a>
                                </li>
                                <li>
                                    <a href="#">4</a>
                                </li>
                                <li>
                                    <a href="#">Next</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>